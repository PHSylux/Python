favorite_color = input('Enter favorite color:\n')
user_name = (input())
user_num = int(input())
print(f'You entered: {favorite_color} {user_name} {user_num}')
# FIXME (2): Output two password options
password1 = (f'{favorite_color}_{user_name}')
password2 = (f'{user_num}{favorite_color}{user_num}')
print(f'\nFirst password: {password1}')
print(f'Second password: {password2}\n')
print(f'Number of characters in {password1}: {len(password1)}')
print(f'Number of characters in {password2}: {len(password2)}') 
